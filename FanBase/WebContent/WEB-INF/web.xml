<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>FanBase</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>mainservlet</servlet-name>
    <servlet-class>
    	com.github.marjan87.fan_base.main.MainServlet
   	</servlet-class>
    <init-param>
      <param-name>secretCode</param-name>
      <param-value>111-11-1111</param-value>
    </init-param>
  </servlet>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>
    	com.github.marjan87.fan_base.main.Login	
   	</servlet-class>
  </servlet>
   <servlet>
    <servlet-name>fanbase</servlet-name>
    <servlet-class>
    	com.github.marjan87.fan_base.main.Fanbase	
   	</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>logout</servlet-name>
    <servlet-class>
    	com.github.marjan87.fan_base.main.Logout	
   	</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>profil</servlet-name>
    <servlet-class>
    	com.github.marjan87.fan_base.main.Profil	
   	</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>register</servlet-name>
    <servlet-class>
    	com.github.marjan87.fan_base.main.Register
   	</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/Login.do</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
    <servlet-name>fanbase</servlet-name>
    <url-pattern>/Fanbase.do</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
    <servlet-name>logout</servlet-name>
    <url-pattern>/Logout.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>mainservlet</servlet-name>
    <url-pattern>/MainServlet.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>register</servlet-name>
    <url-pattern>/Register.do</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>profil</servlet-name>
    <url-pattern>/Profil.do</url-pattern>
  </servlet-mapping>
  <context-param>
    <param-name>pageTitle</param-name>
    <param-value>Fan Base</param-value>
  </context-param>
</web-app>